<section class="panel list-panel tickets full-panel">
<header class="panel-header navbar guide-spot">
<ul class="nav centerNav">
	<%= render 'shared/search' %>
	<%= render 'shared/profile2' %>
</ul>
</header>
<article class="content">

<%= render 'shared/vendor_info' %>

<div class="slimScrollDiv" style="position: relative; overflow: hidden; width: auto; height: auto;">

	<section class="ticketList" style="display: block; overflow: hidden; width: auto; height: auto;">

	<table class="table">
		<tr>
			<th>文件类型：</th>
			<td>
				<select id="method" name="method">
					<option value="0">固件镜像</option>
					<option value="1">Vendor 配置文件</option>
				</select>
			</td>
		</tr>
		<tr>
			<th>描述：</th>
			<td><input type="text" id='file_desc'></td>
		</tr>
		<tr>
			<th>文件位于：</th>
			<td>
				<input type="radio" name="file_type" id="file_type">&nbsp;&nbsp;&nbsp;从列表&nbsp;&nbsp;&nbsp;
				<input type="radio" name="file_type" id="file_type">&nbsp;&nbsp;&nbsp;手动&nbsp;&nbsp;&nbsp;
			</td>
		</tr>
		<tr>
			<th>URL：</th>
			<td>
				<input type="text" id="file_url">
			</td>
		</tr>
		<tr>
			<th>用户名：</th>
			<td>
			<input type="text" id="username">
			</td>
		</tr>
		<tr>
			<th>密码：</th>
			<td>
			<input type="text" id="password">
			</td>
		</tr>
		<tr>
			<th>文件大小：</th>
			<td>
			<input type="text" id="file_size">
			</td>
		</tr>
		<tr>
			<th>延迟：</th>
			<td>
			<input type="text" id="file_delay">
			</td>
		</tr>
	</table>
	<%= render 'shared/bottom_buttom' %>
</section>
</div>
</article>
